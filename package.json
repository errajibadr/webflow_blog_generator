{
  "name": "webflow-blog-generator",
  "version": "1.0.0",
  "description": "Enrich Webflow exports with blog functionality",
  "main": "enrich-webflow-export.js",
  "scripts": {
    "start": "node enrich-webflow-export.js --export ./websites/dogtolib/ --csv ./data/output/ --config ./configs/dogtolib-config.json --output ./dist/dogtolib",
    "serve": "serve dist/dogtolib",
    "watch": "nodemon --watch src --watch enrich-webflow-export.js -e js,html,css,svg,json --exec 'npm start'",
    "dev": "concurrently \"npm run watch\" \"npm run serve\""
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "csv-parse": "^5.5.3",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.9",
    "serve": "^14.2.1"
  },
  "keywords": [
    "webflow",
    "blog",
    "generator",
    "static-site"
  ],
  "author": "",
  "license": "ISC"
}
